{
  "name": "e-shop-server",
  "version": "1.0.0",
  "engines": {
    "node": "11.10.0",
    "npm": "6.13.4"
  },
  "scripts": {
    "client": "cd online-shopping && npm start",
    "server": "nodemon server.js",
    "build": "cd online-shopping && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.16.4",
    "express-sslify": "^1.2.0",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^2.0.2"
  }
}
